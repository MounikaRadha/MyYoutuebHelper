{
    "name": "YoutubeSongsLister",
    "description": "try to list songs in a given video",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_icon": "hello_extensions.png"
    }
    ,
    "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": ["scripting", "activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      }
    }
  }
  }
  